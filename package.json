{
  "scripts": {
    "serve": "web-dev-server --port 8001 --root-dir build",
    "dev": "concurrently -n tsc,wds \"tsc --watch --preserveWatchOutput\" \"npx web-dev-server --port 8001 --watch --root-dir build\"",
    "compile": "tsc",
    "build": "npm run compile && ./build-wasm.sh && npm run build-index",
    "build-index": "node build-index.mjs",
    "build-cached": "npm run compile && npm run build-index",
    "test": "TS_NODE_PROJECT=test/tsconfig.json wdio run test/wdio.conf.ts",
    "test-extra": "TS_NODE_PROJECT=test/tsconfig.json wdio run test/extra.conf.ts"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@jspm/generator": "^1.1.11",
    "@lit-labs/context": "^0.4.1",
    "@types/jasmine": "^4.3.6",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@wdio/cli": "^8.16.11",
    "@wdio/jasmine-framework": "^8.16.11",
    "@wdio/local-runner": "^8.16.11",
    "@wdio/spec-reporter": "^8.16.9",
    "@web/dev-server": "^0.3.1",
    "chromedriver": "^117.0.1",
    "concurrently": "^8.2.1",
    "emscripten-sdk-npm": "github:devappd/emscripten-sdk-npm",
    "eslint": "^8.49.0",
    "eslint-config-google": "^0.14.0",
    "lit": "^2.8.0",
    "prettier": "3.0.3",
    "tree-sitter-cli": "^0.20.8",
    "tree-sitter-markdown": "github:MDeiml/tree-sitter-markdown",
    "ts-lit-plugin": "^1.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wdio-chromedriver-service": "^8.1.1",
    "web-tree-sitter": "^0.20.8",
    "webdriverio": "^8.16.6"
  }
}
