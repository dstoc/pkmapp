{
  "type": "module",
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "dev": "vite dev",
    "preview": "pnpm run vitebuild && vite preview",
    "vitebuild": "vite build --target esnext",
    "check": "tsc --noEmit -p tsconfig.json",
    "update-deps": "./build-wasm.sh && ./build-fonts.sh",
    "build": "pnpm run update-deps && pnpm run vitebuild",
    "build-cached": "pnpm run vitebuild",
    "unittest": "TS_NODE_PROJECT=./tsconfig.test.json wdio run src/wdio.conf.ts",
    "test": "TS_NODE_PROJECT=test/tsconfig.json wdio run test/wdio.conf.ts",
    "test-extra": "TS_NODE_PROJECT=test/tsconfig.json wdio run test/extra.conf.ts"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@fontsource/noto-emoji": "^5.0.17",
    "@lit/context": "^1.1.1",
    "@tree-sitter-grammars/tree-sitter-markdown": "^0.2.3",
    "@types/jasmine": "^5.1.4",
    "@types/trusted-types": "^2.0.7",
    "@types/wicg-file-system-access": "^2023.10.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@wdio/browser-runner": "^8.36.1",
    "@wdio/cli": "^8.36.1",
    "@wdio/globals": "^8.36.1",
    "@wdio/jasmine-framework": "^8.31.1",
    "@wdio/local-runner": "^8.31.1",
    "@wdio/mocha-framework": "^8.36.1",
    "@wdio/spec-reporter": "^8.36.1",
    "chromedriver": "^124.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "firacode": "^6.2.0",
    "lit": "^3.1.2",
    "prettier": "3.2.5",
    "tree-sitter-cli": "^0.22.6",
    "ts-lit-plugin": "^2.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "wdio-chromedriver-service": "^8.1.1",
    "web-tree-sitter": "^0.22.6",
    "webdriverio": "^8.24.1"
  }
}
